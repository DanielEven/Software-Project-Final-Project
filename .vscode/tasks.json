{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-I/usr/include/python3.8",
                "../*.c",
                "${file}",
                "-lm",
                "-g",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-L/usr/lib/x86_64-linux-gnu/libpython3.8.so",
                "-lpython3.8",
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}